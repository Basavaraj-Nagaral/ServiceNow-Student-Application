        <script><![CDATA[function onSubmit() {
   //Type appropriate comment here, and begin script below
   var a = g_form.getValue('physics');
var b = g_form.getValue('chemistry');
	var c = g_form.getValue('mathematics');
	var d = g_form.getValue('u_integer_8');
	var total = (parseInt(a) + parseInt(b) + parseInt(c) + parseInt(d));
	var sum = total/400;
	var percent = sum * 100;
	g_form.setValue('percentage',percent,'%');
	
	if(percent < 30){
		g_form.setValue('result','fail');
	}
	else{
		g_form.setValue('result','pass');
	}
	

	
	g_form.addInfoMessage(total);
}]]></script>
